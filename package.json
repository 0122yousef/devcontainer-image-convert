{
	"name": "devcontainer-image-convert",
	"displayName": "Image to Dockerfile",
	"description": "Convert the \"image\" property in a devcontainer.json to a Dockerfile.",
	"version": "0.0.1",
	"publisher": "Brigit",
	"icon": "media/dev-container-org-arrow.png",
	"repository": {
		"type": "git",
		"url": "https://github.com/bamurtaugh/devcontainer-image-convert"
	},
	"bugs": {
		"url": "https://github.com/bamurtaugh/devcontainer-image-convert/issues"
	},
	"engines": {
		"vscode": "^1.73.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onLanguage:jsonc"
	],
	"main": "./out/extension.js",
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"lint": "eslint \"src/**/*.ts\"",
		"watch": "tsc -watch -p ./"
	},
	"devDependencies": {
		"@types/node": "^16.11.7",
		"@types/vscode": "^1.73.0",
		"@typescript-eslint/eslint-plugin": "^5.42.0",
		"@typescript-eslint/parser": "^5.42.0",
		"eslint": "^8.26.0",
		"typescript": "^4.9.4"
	}
}
